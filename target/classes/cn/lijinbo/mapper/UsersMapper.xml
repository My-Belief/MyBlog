<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.lijinbo.mapper.UsersMapper" >
  <resultMap id="BaseResultMap" type="cn.lijinbo.pojo.Users" >
    <id column="id" property="id" jdbcType="INTEGER" />
    <result column="account" property="account" jdbcType="VARCHAR" />
    <result column="password" property="password" jdbcType="VARCHAR" />
  </resultMap>
  <select id="selectUserByAccount" resultMap="BaseResultMap"  parameterType="String" >
        select id,account,password from tb_my_account where account = #{account}
  </select>
</mapper>